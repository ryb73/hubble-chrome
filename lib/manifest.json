{
    "manifest_version": 2,

    "name": "Hubble",
    "description": "Make GitHub better.",
    "version": "0.1",

    "content_scripts": [
        {
            "matches": [ "*://github.com/*" ],
            "js": [ "js/content.js" ]
        }
    ],

    "permissions": [
        "https://*.github.com/*"
    ]
}
{
  "name": "upgraded-waffle",
  "version": "1.0.0",
  "description": "Make GitHub better.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "tdb": "node debug ./node_modules/.bin/_mocha --compilers js:babel-register",
    "build": "webpack --progress --config webpack/webpack.config.js",
    "watch": "webpack --progress --config webpack/webpack.config.js --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "author": "Ryan Biwer",
  "license": "MIT",
  "repository": "ryb73/upgraded-waffle",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.16.3",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "3.0.1",
    "eslint": "3.6.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "flow-bin": "^0.33.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "github-injection": "0.2.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.0",
    "url-parse": "^1.1.3",
    "webpack": "2.1.0-beta.22"
  }
}
